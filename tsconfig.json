{
  "include": ["src"],
  "compilerOptions": {
    "strict": true,
    "sourceMap": true,
    "module": "esnext",
    "target": "es6",
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": ["vite/client", "node"],
    "baseUrl": "src"
  }
}
